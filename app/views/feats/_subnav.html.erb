<ul class="navigation primary">
  <% Tag.of_kind("Feat").each do |type| %>
    <li class="<%= active_li_css(@view_by, type.name) %>">
			<small><%=
				link_to_unless(
				  (@view_by == type.name),
					type.name,
					:controller => 'feats',
					:action => 'index',
					:type => type.name
				)
			%></small>
		</li>
	<% end %>
</ul>
<div class="clear"></div>